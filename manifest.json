{
  "name": "print-all-tabs",
  "description": "Select and print content from multiple chrome tabs at once",
  "version": "0.1",
  "manifest_version": 2,

  "description": "A plain text description",
  "icons": { "16": "printer-16.png", "128": "printer-128.png" },
  "browser_action": {
    "name": "Click to start app"
  },
  "background": {
    "scripts": [ "background.js" ]
    },
  "permissions": ["tabs"]

}
